<?xml version="1.0" encoding="UTF-8"?>
<extentreports>

	<configuration>
		<!-- report theme -->
		<!-- standard, dark -->
		<theme>dark</theme>

		<!-- document encoding -->
		<!-- defaults to UTF-8 -->
		<encoding>UTF-8</encoding>

		<!-- protocol for script and stylesheets -->
		<!-- defaults to https -->
		<protocol>https</protocol>

		<!-- title of the document -->
		<documentTitle>OxfordDictionaryAPI Test Report</documentTitle>

		<!-- report name - displayed at top-nav -->
		<reportName>OxfordDictionaryAPI Test Automation Report</reportName>

		<!-- report headline - displayed at top-nav, after reportHeadline -->
		<reportHeadline></reportHeadline>

		<!-- global date format override -->
		<!-- defaults to yyyy-MM-dd -->
		<dateFormat>yyyy-MM-dd</dateFormat>

		<!-- global time format override -->
		<!-- defaults to HH:mm:ss -->
		<timeFormat>HH:mm:ss</timeFormat>

		<!-- custom javascript -->
		<scripts>
<![CDATA[
                $(document).ready(function() {
                    
                });
            ]]>
		</scripts>

		<!-- custom styles -->
		<styles>
			<style>
				.zoom {
				padding:0px;

				transition: transform .2s; /* Animation */
				width: 300px;

				margin: 0 auto;
				}

				.zoom:hover {
				transform: scale(3.5); /*
				(150% zoom - Note:
				if the zoom is too large, it will
				go outside of the
				viewport) */
				}
			</style>
		</styles>
	</configuration>
</extentreports>